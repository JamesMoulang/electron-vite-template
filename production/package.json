{
  "name": "electron-vite-template-production",
  "productName": "Product Name",
  "savefilename": "kana_save",
  "devTools": true,
  "version": "1.0.0",
  "appBundleId": "com.jamesmoulang.you-can-kana-mac",
  "description": "A blank project for an electron application inc. notarization",
  "main": "main.js",
  "build": {
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build_assets/mac/icon.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build_assets/mac/entitlements.plist",
      "entitlementsInherit": "build_assets/mac/entitlements.plist"
    },
    "afterSign": "./scripts/notarize.js"
  },
  "scripts": {
    "start": "electron .",
    "app:dist_final": "electron-builder --config.extraMetadata.devTools=false",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder --config.extraMetadata.devTools=true"
  },
  "author": "James Moulang",
  "devDependencies": {
    "electron": "^24.1.3",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.2",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "steamworks.js": "0.1.0"
  }
}
